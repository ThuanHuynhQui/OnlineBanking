@model IEnumerable<OnlineBanking.Models.Cheques>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ChequeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CardId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Leaf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChequeTypeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Active)
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ChequeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CardId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Leaf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChequeTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ChequeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ChequeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ChequeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

@model IEnumerable<OnlineBanking.Models.ChequeTypes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ChequeTypeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChequeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ChequeTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChequeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ChequeTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ChequeTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ChequeTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
